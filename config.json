{
  "data": {
    "emojis_dir": "data/emojis",
    "canvas": {
      "P": 8,
      "H": 32,
      "W": 32
    },
    "targets": [
      "bacteria.png",  
      "tooth.png",   
      "heart.png",   
      "squid.png",  
      "smile.png",
      "eye.png",
      "mushroom.png",
      "four_leaf_clover.png",
      "gecko.png",
      "star.png"
    ]
  },

  "model": {
    "channels": { "C": 16, "d": 32 },
    "iterations": { "K": 32 },
    "attention": {
      "type": "concat_mlp",
      "mlp_hidden": 64,
      "mlp_layers": 2
    },
    "convgru": { "kernel_size": 3 }
  },

  "training": {
    "batch_size": 1,
    "num_epochs": 40,
    "learning_rate": 0.001,
    "optimizer": "Adam",
    "weight_decay": 0.0001,
    "loss": "mse"
  },

  "logging": {
    "log_interval": 10,
    "save_interval": 100,
    "results_dir": "results"
  },

  "seed": 42
}
